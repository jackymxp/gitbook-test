, const int lo, const int hi) {\n        for (int i = hi; i >= lo + 1; i--) {\n            if (bubbleOperatorAdvanced(arr, i))\n                return;\n            // assert(isSorted(arr, i, hi));\n        }\n    }\n\n    //在arr[0...hi]做冒泡过程，将最大值放在arr[hi]位置\n    inline void bubbleOperator(T *arr, const int hi) {\n        for (int i = 0; i  arr[i + 1])\n                swap(arr, i, i + 1);\n    }\n\n    //没有发生交换返回true\n    inline bool bubbleOperatorAdvanced(T *arr, const int hi) {\n        bool isSort = true;\n        for (int i = 0; i  arr[i + 1]) {\n                swap(arr, i, i + 1);\n                isSort = false;\n            }\n        }\n        return isSort;\n    }\n\n    void swap(T *arr, const int i, const int j) {\n        T k = arr[i];\n        arr[i] = arr[j];\n        arr[j] = k;\n    }\n\n    bool isSorted(T *arr, const int lo, const int hi) {\n        for (int i = lo; i  arr[i + 1])\n                return false;\n        return true;\n    }\n\n    bool isSorted(T *arr, const int len) {\n        return isSorted(arr, 0, len - 1);\n    }\n};\n\nint main(void){\n    srand(time(NULL));\n    BubbleSort bubbleSort;\n    const int len = 10000;\n    int* arr = new int[len];\n\n\n    for(int i = 0; i \n\n\n\n       window.klipse_settings = {         selector: \".language-klipse, .lang-eval-clojure\",         selector_eval_js: \".lang-eval-js\",         selector_eval_python_client: \".lang-eval-python\",         selector_eval_php: \".lang-eval-php\",         selector_eval_scheme: \".lang-eval-scheme\",         selector_eval_ruby: \".lang-eval-ruby\",         selector_reagent: \".lang-reagent\",        selector_google_charts: \".lang-google-chart\",        selector_es2017: \".lang-eval-es2017\",        selector_jsx: \".lang-eval-jsx\",        selector_transpile_jsx: \".lang-transpile-jsx\",        selector_render_jsx: \".lang-render-jsx\",        selector_react: \".lang-react\",        selector_eval_markdown: \".lang-render-markdown\",        selector_eval_l